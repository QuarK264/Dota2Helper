@using Dota2Helper.Models.ViewModels
@model EnemysBlockLSPartialViewModel

<table>
    @foreach (EnemyBlockLSViewModel enemy in Model.Enemys)
    {
        <tr>
            @foreach (SkillLSViewModel skill in enemy.EnemySkills.Where(skill => skill.SkillName != null))
            {
                <td>
                    <div class="substrate"></div>
                    <img src="@skill.ImageLink" alt="@skill.PoolNumber" />
                </td>
            }
        </tr>
    }
</table>